{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "parser": "babel-eslint"
    },

    "env": {
        "browser": true,
        "es6": true
    },

    "extends": [
        "eslint:recommended"
    ],

    "globals" : {
        "data" :"writable",
        "imgSelected" :"writable",
        "idPhotographer" :"writable",
        "photographerDatas" :"writable",
        "mediaDatas" :"writable",
        "currentIndex" :"writable",
        "nextIndex" :"writable",
        "previousIndex" :"writable",
        "slidePrevious" : "writable",
        "slideNext" : "writable",
        "photographerFactory": true,
        "mediaFactory" : true,
        "displayGalleryPhotographer": true,
        "closeFilterMenu" : true,
        "main" : "readonly"
    },

    "rules": {
        "eqeqeq": "off",
        "indent": ["warn", 4, { "SwitchCase": 1 }],
        "multiline-ternary": ["warn", "always-multiline"],
        "no-unused-vars": "off",
        "quotes": ["error", "double"],
        "no-control-regex": "error"
       
    }
  
    
  }